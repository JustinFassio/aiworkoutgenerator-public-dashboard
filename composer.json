{
    "name": "athlete-dashboard/child-theme",
    "description": "Athlete Dashboard Child Theme",
    "type": "wordpress-theme",
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "brain/monkey": "^2.6",
        "mockery/mockery": "^1.5",
        "10up/wp_mock": "^0.4.2"
    },
    "autoload": {
        "psr-4": {
            "AthleteDashboard\\": "features/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AthleteDashboard\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:unit": "phpunit --testsuite unit",
        "test:integration": "phpunit --testsuite integration"
    }
} 